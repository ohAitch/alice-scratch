<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<false/>
	<key>EnableGlobbing</key>
	<false/>
	<key>ExitTimeOut</key>
	<integer>600</integer>
	<key>Label</key>
	<string>local.chrometabs</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Applications/Google Chrome.app/Contents/MacOS/Google Chrome</string>
		<string>https://calendar.google.com/calendar/render#main_7%7Ccustom,5</string>
		<string>https://gmail.com/</string>
		<string>https://facebook.com/messages</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StartCalendarInterval</key>
	<array>
		<dict>
			<key>Hour</key>
			<integer>7</integer>
			<key>Minute</key>
			<integer>0</integer>
		</dict>
	</array>
</dict>
</plist>
