has_prop ← (o,m)=> Object.getOwnPropertyDescriptor(o,m) !== undefined

css ← ι=>{ t ← document.createElement('style'); t.innerHTML = ι; document.head.appendChild(t) }
mk_script ← ι=>{ t ← document.createElement('script'); Object.assign(t,ι); document.getElementsByTagName('head')[0].appendChild(t) }

