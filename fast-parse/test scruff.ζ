p ← P([ P.alt([…/[a-z]/]).+.map(ι=> ι.join('')), ':', P.alt([…/[ -~]/]).+.map(ι=> ι.join('')) ])
cn.log('a',p.exec('name: also name'))
p ← P([ '"', P.alt([…/[ -~]/]).*.map(ι=> ι.join('')), '"' ])
cn.log('b',p.exec('"foo","bar"'))
p ← P([ '"', P.alt([…/[A-~]/]).*.map(ι=> ι.join('')), '"' ])
cn.log('c',p.exec('"foo","bar"'))
p ← P([ '"', P.alt([…/[ -~]/]).*?.map(ι=> ι.join('')), '"' ])
cn.log('d',p.exec0('"foo","bar"'))
cn.log('e',p.exec0('""'))
cn.log('f',p.exec0('"foo"'))
p ← P([ '"', P.alt([…/[ -~]/]).repeat([0,4]).map(ι=> ι.join('')), '"' ])
cn.log('g',p.exec('"foo","bar"'))
