#!/usr/bin/env ζ
read_stdin ← λ(f){process.stdin.read_all(λ(e,ι){f((ι+'').replace(/\n$/,''))})}
read_stdin(λ(ι){

t←[]; ι.split('').sort()._.uniq(true).map(λ(ι){ι = ι.charCodeAt(); t[-1]&&t[-1][1] === ι-1? (t[-1][1] = ι) : t.push([ι,ι])})
x ← λ(ι){ι = String.fromCharCode(ι); ↩ {'-':1,']':1,'\\':1}[ι]? '\\'+ι : ι==="'"? ι : util.inspect(ι).slice(1,-1).replace(/\\u00/g,'\\x')}
ι = t.map(λ(ι){↩ ι[0] === ι[1]? x(ι[0]) : ι[0] === ι[1]-1? x(ι[0])+x(ι[1]) : x(ι[0])+'-'+x(ι[1])}).join('')
process.stdout.write(ι)

})
